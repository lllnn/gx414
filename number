void CMFCApplication1Dlg::OnBnClickedButtonwhat()
{
	// TODO: 在此添加控件通知处理程序代码
	UpdateData(TRUE);
	if (str != "0")
		str = str + _T(")");
	else
		str = ")";
	UpdateData(FALSE);
}


